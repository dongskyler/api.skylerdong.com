    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.1",
    "express": "~4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.0.0",
    "http-errors": "~1.7.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.18",
    "morgan": "~1.10.0",
    "npm-run-all": "^4.1.5",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
